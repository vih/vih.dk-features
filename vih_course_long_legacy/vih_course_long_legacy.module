/**
 * Module shows lists with long courses. The single courses
 * will not be shown here, but in a separate registration system.
 */

/**
 * Implementation of hook_views_api().
 */
function vih_course_long_legacy_views_api() {
    return array(
        'api' => 3,
        'path' => drupal_get_path('module', 'vih_course_long_legacy'),
    );
}

